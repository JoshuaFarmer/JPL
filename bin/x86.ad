TOK_ADD@  add eax,ebx
TOK_SUB@  sub eax,ebx
TOK_IF@  jnz M%d
TOK_ENDIF@M%d:
TOK_SUBR@  jmp $\S%s:
TOK_ENDSUB@  ret
TOK_CALL@  call S%s
TOK_VAR_GET_1@  mov eax,[vars+%d]
TOK_VAR_GET_2@  mov ebx,[vars+%d]
TOK_NUM_1@  mov eax,%d
TOK_NUM_2@  mov ebx,%d
TOK_SET@  mov dword[vars+%d],eax
TOK_ADD_SET@  add dword[vars+%d],eax
TOK_SUB_SET@  sub dword[vars+%d],eax
TOK_ISEQUAL@  cmp eax,ebx\  setz al
HLT@  jmp $