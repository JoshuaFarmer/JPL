TOK_ADD@  add eax,ebx
TOK_SUB@  sub eax,ebx
TOK_IF@  test al,al\  jz M%d
TOK_ENDIF@M%d:
TOK_SUBR@  jmp $\S%s:
TOK_ENDSUB@  ret
TOK_CALL@  call S%s
TOK_VAR_GET_1@  mov eax,[%d]
TOK_VAR_GET_2@  mov ebx,[%d]
TOK_NUM_1@  mov eax,%d
TOK_NUM_2@  mov ebx,%d
TOK_SET@  mov dword[%d],eax
TOK_ADD_SET@  add dword[%d],eax
TOK_SUB_SET@  sub dword[%d],eax
TOK_ISEQUAL@  cmp eax,ebx\  setz al
HLT@  jmp $